{
    "name": "docs",
    "private": true,
    "description": "Personal documentation hub formatting tooling",
    "license": "MIT",
    "devDependencies": {
        "markdownlint-cli2": "0.20.0",
        "prettier": "3.8.1"
    },
    "scripts": {
        "format:md": "prettier --write \"**/*.md\"",
        "format:md:check": "prettier --check \"**/*.md\"",
        "lint:md": "markdownlint-cli2 \"**/*.md\" --config .markdownlint.json"
    }
}
